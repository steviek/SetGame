<?xml version="1.0" encoding="utf-8"?>
<resources>
    
    <declare-styleable name="SetCardView">
        <attr name="itemNum" format="enum">
            <enum name="one" value="1"/>
            <enum name="two" value="2"/>
            <enum name="three" value="3"/>
        </attr>
        <attr name="itemColor" format="enum">
            <enum name="red" value="1"/>
            <enum name="purple" value="2"/>
            <enum name="green" value="3"/>
        </attr>
        <attr name="itemShading" format="enum">
            <enum name="solid" value="1"/>
            <enum name="striped" value="2"/>
            <enum name="open" value="3"/>
        </attr>
        <attr name="itemShape" format="enum">
            <enum name="oval" value="1"/>
            <enum name="diamond" value="2"/>
            <enum name="squiggle" value="3"/>
        </attr>
    </declare-styleable>
    
</resources>